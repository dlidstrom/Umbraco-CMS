<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Title>Umbraco CMS - Persistence - SQLite</Title>
    <Description>Adds support for PostgreSQL to Umbraco CMS.</Description>

    <!-- remove if this package is made into a NuGet package -->
    <EnablePackageValidation>false</EnablePackageValidation>
